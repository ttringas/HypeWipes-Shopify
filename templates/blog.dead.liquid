		
		<!-- PRIMARY -->
		<div id="primary_column" class="col66 right">
			<!-- START CONTENT -->
			
			{% paginate blog.articles by 10 %}
			
			{% for article in blog.articles %}
			
			<!-- START ENTRY {{ forloop.index }} -->
			<div class="entry{% if forloop.first %} start{% endif %}{% if forloop.last %} end{% endif %}">
				<!-- START TITLE -->
				<div class="title">
					<h2><a href="{{ article.url }}">{{ article.title | escape }}</a></h2>
					<h5>Posted {{ article.created_at | date: "%B %d, %Y" }} in <span>(<a href="{{ article.url }}{% if blog.comments_enabled? %}#comments{% endif %}">{{ article.comments_count }} {{ article.comments_count | pluralize: "Comment", "Comments" }}</a>)</span></h5>
				</div>
				<!-- END TITLE -->
				<!-- START BODY -->
				<div class="body">
					{{ article.content }}
					<p><a href="{{ article.url }}">Continue...</a></p>
				</div>
				<!-- END BODY -->
			</div>
			<!-- END ENTRY {{ forloop.index }} -->{% endfor %}{% if paginate.pages > 1 %}
			{% include "pagination" %}
			{% endif %}
			{% endpaginate %}



				
			<!-- END CONTENT -->
		</div>
		<!-- / PRIMARY -->
		
		
		<!-- ASIDE -->
		<div id="home_side" class="col33 left">
					
			{% include "recent_posts" %}
					
			{% include "post_tags" %}
			
			{% include 'twitter' %}
			
		</div>
		<!-- /  ASIDE -->